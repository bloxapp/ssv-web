  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "cross-env NODE_ENV=development NODE_OPTIONS=--openssl-legacy-provider INLINE_RUNTIME_CHUNK=false node scripts/start.js",
    "build": "cross-env INLINE_RUNTIME_CHUNK=false NODE_OPTIONS=--openssl-legacy-provider node --max_old_space_size=8192 scripts/build.js",
    "test": "cross-env NODE_ENV=test INLINE_RUNTIME_CHUNK=false node scripts/test.js --watchAll=false --verbose --detectOpenHandles --forceExit",
    "test:dev": "cross-env NODE_ENV=test INLINE_RUNTIME_CHUNK=false node scripts/test.js --coverage --reporters=default",
    "cy:open": "yarn local-node && cross-env NODE_ENV=test cypress open --config-file cypress.json",
    "cy:open:local": "yarn local-node && cross-env NODE_ENV=test cypress open --config-file cypress.json --config baseUrl=http://localhost:3000",
    "cy:headless": "cross-env NODE_ENV=test cypress run --config-file cypress-headless.json",
    "lint": "eslint 'src/**'",
    "postinstall": "link-module-alias && husky install",
    "preinstall": "command -v link-module-alias && link-module-alias clean || true",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "local-node": "./scripts/start_local_node.sh",
    "local-server": "./scripts/start_webapp_for_tests.sh",
    "security:check": "./scripts/check_security.sh",
    "prod:watch": "nodemon -w ./src --ext js,jsx,json,css,scss --exec \"yarn build && serve -s build\""
  },
  "license": "GPL-3.0",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "_moduleAliases": {
    "~root": "./src",
    "~app": "./src/app",
    "~lib": "./src/lib",
    "~config": "./config"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testRunner": "<rootDir>/node_modules/jest-circus/runner.js",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true,
    "testEnvironment": "~config/jest/TestsEnvironments",
    "testTimeout": 10000
  },
  "devDependencies": {
    "@testing-library/dom": "^7.30.4",
    "@types/bs58check": "^2.1.0",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^24.0.0",
    "@types/jsdom": "^16.2.10",
    "@types/jsdom-global": "^3.0.2",
